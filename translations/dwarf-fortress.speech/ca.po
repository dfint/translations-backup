msgstr "Durant [CONTEXT:NUMBER] dels meus anys, he rescatat nens perduts i els he portat de tornada a [CONTEXT:PLACE:TRANS_NAME]."

#: soldier_profession.txt:1
msgid "I am a soldier."
msgstr "Sóc un soldat."

#: task_recommendation.txt:1
msgid "[CONTEXT:ANY:TRANS_NAME] might have a task for you"
msgstr "[CONTEXT:ANY:TRANS_NAME] podria tenir feina per a tu."

