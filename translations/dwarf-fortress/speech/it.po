msgstr "Per [CONTEXT:NUMBER] dei miei anni, ho salvato bamini perduti e li ho riportati a [CONTEXT:PLACE:TRANS_NAME]."

#: soldier_profession.txt:1
msgid "I am a soldier."
msgstr "Sono un soldato."

#: task_recommendation.txt:1
msgid "[CONTEXT:ANY:TRANS_NAME] might have a task for you"
msgstr "[CONTEXT:ANY:TRANS_NAME] potrei avere un compito per te"

#: temple_already_member.txt:1
msgid "simply"
msgstr "semplicemente"

#: temple_already_member.txt:2
msgid "it is enough to"
msgstr "è abbastanza per"

#: temple_already_member.txt:3
msgid "your task is simple;"
msgstr "il tuo compito è semplice"

#: temple_already_member.txt:4
msgid "just"
msgstr "solo"

#: temple_already_member.txt:5
