msgstr "[C:7:0:1]Butchering[C:7:0:0]"

#: stringdump_steam_0_50_06_sorted_by_xrefs.txt:2332
msgid ""
"[C:6:0:1]Meat[C:7:0:0] from [C:2:0:0]Livestock[C:7:0:0] you slaughter is a "
"source of food. "
msgstr ""
"[C:6:0:1]Meat[C:7:0:0] from [C:2:0:0]Livestock[C:7:0:0] you slaughter is a "
"source of food. "

#: stringdump_steam_0_50_06_sorted_by_xrefs.txt:2333
