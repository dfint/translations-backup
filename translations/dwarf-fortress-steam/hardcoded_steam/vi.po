msgstr "Số lượng Ô địa hình có độ cao trung bình tối thiểu"

msgid "Minimum Number of Low-Elevation Squares"
msgstr "Số lượng Ô địa hình có độ cao thấp tối thiểu"

msgid "Minimum Number of High-Elevation Squares"
msgstr "Số lượng Ô địa hình có độ cao cao tối thiểu"

msgid "Minimum Number of Mid-Rain Squares"
msgstr "Số lượng Ô địa hình có lượng mưa trung bình tối thiểu"

msgid "Minimum Number of Low-Rain Squares"
msgstr "Số lượng Ô địa hình có lượng mưa thấp tối thiểu"

msgid "Minimum Number of High-Rain Squares"
msgstr "Số lượng Ô địa hình có lượng mưa cao tối thiểu"

msgid "Minimum Number of Mid-Drainage Squares"
msgstr "Số lượng Ô địa hình có độ thoát nước trung bình tối thiểu"

msgid "Minimum Number of Low-Drainage Squares"
msgstr "Số lượng Ô địa hình có độ thoát nước thấp tối thiểu"

msgid "Minimum Number of High-Drainage Squares"
msgstr "Số lượng Ô địa hình có độ thoát nước cao tối thiểu"

msgid "Minimum Number of Mid-Savagery Squares"
msgstr "Số lượng Ô địa hình có độ hoang dã trung bình tối thiểu"

msgid "Minimum Number of Low-Savagery Squares"
msgstr "Số lượng Ô địa hình có độ hoang dã thấp tối thiểu"

msgid "Minimum Number of High-Savagery Squares"
msgstr "Số lượng Ô địa hình có độ hoang dã cao tối thiểu"

msgid "Minimum Number of Mid-Volcanism Squares"
msgstr "Số lượng Ô địa hình có hoạt động núi lửa trung bình tối thiểu"

msgid "Minimum Number of Low-Volcanism Squares"
msgstr "Số lượng Ô địa hình có hoạt động núi lửa thấp tối thiểu"

msgid "Minimum Number of High-Volcanism Squares"
msgstr "Số lượng Ô địa hình có hoạt động núi lửa cao tối thiểu"

msgid "LARGE ISLAND"
msgstr "ĐẢO LỚN"

msgid "LARGE REGION"
