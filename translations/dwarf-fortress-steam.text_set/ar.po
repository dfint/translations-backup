#: text_family_relationship_additional_dead.txt:2
msgid "[CONTEXT:HIST_FIG:PRO_SUB] was also my [CONTEXT:FAMILY_RELATIONSHIP]"
msgstr "[CONTEXT:HIST_FIG:PRO_SUB] كان ايضا [CONTEXT:FAMILY_RELATIONSHIP]"

